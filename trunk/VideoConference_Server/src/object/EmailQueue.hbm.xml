<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
              "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">       
<hibernate-mapping>
           <class name="object.Conference" table="tbl_vc_conference">
				<id name="email_queue_id" type="java.lang.Integer">
					<column name="email_queue_id"/>
					<generator class="identity"/>
				</id>
				<property name="from" type="java.lang.String">
				     <column name="from"/>
				</property>
				<property name="to" type="java.lang.String">
				    <column name="to"/>
				</property>
				<property name="message" type="java.lang.String">
				    <column name="message"/>
				</property>
				<property name="subject" type="java.lang.String">
				    <column name="subject"></column>
				</property>
				<property name="headers" type="java.lang.String">
				    <column name="headers"></column>
				</property>
				<property name="cc" type="java.lang.String">
				    <column name="cc"></column>
				</property>
				<property name="bcc" type="java.lang.String">
				    <column name="bcc"></column>
				</property>
				<property name="descriptions" type="java.lang.String">
				     <column name="descriptions"/>
				</property>
				<property name="ip_add" type="java.lang.String">
				     <column name="ip_add"/>
				</property>
				<property name="status" type="java.lang.Integer">
				     <column name="status"/>
				</property>
            </class>
</hibernate-mapping>