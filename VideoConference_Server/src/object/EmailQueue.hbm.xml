<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
              "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">       
<hibernate-mapping>
           <class name="object.EmailQueue" table="tbl_vc_emailqueue">
				<id name="email_queue_id" type="java.lang.Integer">
					<column name="email_queue_id"/>
					<generator class="identity"/>
				</id>
				<property name="fromemail" type="java.lang.String">
				     <column name="fromemail"/>
				</property>
				<property name="toemail" type="java.lang.String">
				    <column name="toemail"/>
				</property>
				<property name="message" type="java.lang.String">
				    <column name="message"/>
				</property>
				<property name="subject" type="java.lang.String">
				    <column name="subject"></column>
				</property>
<property name="status" type="java.lang.Integer">
				     <column name="status"/>
				</property>
            </class>
</hibernate-mapping>